Program -> DeclarationList
DeclarationList -> Declaration DeclarationListPr
DeclarationListPr -> Declaration DeclarationListPr | EPSILON
Declaration -> VarDeclaration | FunDeclaration
VarDeclaration -> TypeSpecifier id ; | TypeSpecifier [ num ] ;
TypeSpecifier -> int | float | void
FunDeclaration -> TypeSpecifier id ( Params ) CmpdStatement
Params  -> ParamList | void
ParamList -> Param ParamListPr
ParamListPr -> , Param ParamListPr | EPSILON
Param -> TypeSpecifier id | TypeSpecifier id [ ]
CmpdStatement -> { LocalDeclaration StatementList }
LocalDeclaration -> VarDeclaration LocalDeclaration | EPSILON
StatementList -> Statement StateList | EPSILON
Statement -> ExpressionStatement | CmpdStatement | SelectionStatement | IterationStatement | ReturnStatement
ExpressionStatement -> Expression ; | ;
SelectionStatement -> if ( Expression ) Statement | if ( Expression ) Statement else Statement
IterationStatement -> while ( Expression ) Statement
ReturnStatement -> return ; | return Expression ;
Expression -> Var = Expression | SimpleExpression
Var -> id | id [ Expression ]
SimpleExpression -> AddExpression RelOp AddExpression
RelOp -> >= | <= | > | < | == | !=
AddExpression -> Term AddExpressionPr
AddExpressionPr -> AddOp Term AddExpressionPr | EPSILON
AddOp -> + | -
Term -> Factor TermPr
TermPr -> MulOp Factor TermPr | EPSILON
MulOp -> * | /
Factor -> ( Expression ) | Var | Call | num
Call -> id ( Args )
Args -> ArgsList | EPSILON
ArgsList -> Expression | ArgsListPr
ArgsListPr -> , Expression ArgsListPr | EPSILON
